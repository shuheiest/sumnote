{
  "name": "sumnote-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend for voice and document sharing portal",
  "scripts": {
    "start": "vite",
    "notios": "notios dev",
    "dev": "run-p dev:*",
    "dev:client": "vite",
    "dev:server": "cd ../backend && npm run dev",
    "dev:aspida": "aspida --watch",
    "dev:typecheck": "tsc --noEmit -w",
    "build": "npm run build:aspida && vite build",
    "build:aspida": "aspida --build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@aspida/axios": "^1.13.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.13.0",
    "@mui/material": "^5.13.0",
    "aspida": "^1.13.0",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.0",
    "react-pdf": "^7.2.0",
    "react-router-dom": "^6.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.0",
    "vite": "^4.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
